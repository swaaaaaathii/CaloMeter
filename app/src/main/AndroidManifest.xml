<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.calometer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="Login" />
        <activity
            android:name=".SignupActivity"
            android:label="Set Your Goal" />
        <activity
            android:name=".ExerciseActivity"
            android:label="Exercise Tracker" />
        <activity
            android:name=".DailyActivity"
            android:label="Daily Accomplishment" />
        <activity
            android:name=".MenuActivity"
            android:label="Calorie Counter" />
        <activity
            android:name=".FirstSignUpActivity"
            android:label="Sign Up" />
        <activity android:name=".FoodDiary" />
        <activity android:name=".GraphActivity"></activity>
    </application>

</manifest>